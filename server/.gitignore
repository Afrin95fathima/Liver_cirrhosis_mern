# Server-specific .gitignore

# Dependencies
node_modules/

# Environment files (CRITICAL - Never commit sensitive data)
.env
.env.local
.env.development.local
.env.test.local
.env.production.local

# Logs
logs/
*.log
npm-debug.log*
yarn-debug.log*
yarn-error.log*
lerna-debug.log*

# Runtime data
pids/
*.pid
*.seed
*.pid.lock

# Coverage directory used by tools like istanbul
coverage/
*.lcov

# Test coverage
.nyc_output/

# Build outputs
dist/
build/

# File uploads directory
uploads/
temp/
tmp/

# Database dumps and backups
*.sql
*.sqlite
*.db
backups/

# Cache directories
.cache/
.npm/
.eslintcache

# Optional dependencies
.optional/

# Production files
production/

# SSL certificates (if any)
*.pem
*.key
*.crt

# Session files
sessions/

# Editor files
.vscode/
.idea/
*.swp
*.swo